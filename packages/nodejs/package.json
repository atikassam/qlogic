{
  "name": "@qlogic/nodejs",
  "version": "2.12.0",
  "dependencies": {
    "jsdom": "^25.0.1"
  },
  "peerDependencies": {
    "@qlogic/react": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "packages/nodejs/src",
    "projectType": "library",
    "name": "nodejs"
  },
  "exports": {
    "./package.json": "./package.json",
    "./node": "./dist/index.cjs.js",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.cjs.js",
      "default": "./dist/index.cjs.js"
    }
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "url": "https://github.com/atikassam/qlogic"
  }
}
